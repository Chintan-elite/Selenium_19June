package fileio;

import java.io.File;

public class F005_File {

		public static void main(String[] args) {
			
			
			File file = new File("D://test//sample.txt");
			
			
			if(file.exists())
			{
				System.out.println("File available");
			}
			else
			{
				System.out.println("File not exist !!!");
			}
			
			
			
		}
}
