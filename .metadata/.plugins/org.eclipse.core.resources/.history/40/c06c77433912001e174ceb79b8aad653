		int b = 20;
		
		String s1 = "Nikunj";
		String s2 = "Taushif";
//		int c = a+b;
//		System.out.println(c);
		
		System.out.println(a+b);
		System.out.println(s1+s2);
		
		System.out.println(a+b+"abc");
		System.out.println("abc"+(a+b));
		
		
		
	}
}
