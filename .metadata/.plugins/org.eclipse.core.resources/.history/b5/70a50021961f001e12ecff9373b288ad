package oops;

class Stud
{
	private int id;
	private String name;
	
	public void display()
	{
		System.out.println(id+" "+name);
	}
}

public class O004_EncaptulationDemo {
	
	public static void main(String[] args) {
		
		Stud std = new Stud();
		std.id=10;
		std.name="Taushif";
		std.display();
		
		
	}
}
