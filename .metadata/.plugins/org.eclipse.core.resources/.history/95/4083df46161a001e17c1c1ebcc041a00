package oops;

class Student
{
	int id;
	String name;
	
	Student()
	{
		System.out.println("Constructor calling...");
	}
	
	public void print()
	{
		System.out.println(id+" "+name);
	}
	
	
}

public class O002_Second {
	public static void main(String[] args) {
		
		Student st = new Student();
		//st.print();
		
		
		
	}
}
