package oops;

class Stud
{
	private int id;
	private String name;
	
	public void setData(int id, String name)
	{
		this.id = id;
		this.name = name;
	}
	
	public void display()
	{
		System.out.println(id+" "+name);
	}
}

public class O004_EncaptulationDemo {
	
	public static void main(String[] args) {
		
		Stud std = new Stud();
		std.setData(10, "Nikunj");
		std.display();
		
		
	}
}
