package array;

public class A003_ArrayOpration {
	public static void main(String[] args) {
		
		int a[] = {2,10,4,1,22,55,89,11,23};
		
		int sum = 0;
		for(int i=0;i<a.length;i++)
		{
			//sum = sum +a[i];
			sum += a[i]; //shorthand oprator
		}
		
		System.out.println("Sum is : "+sum);
		
		
		
		
		
	}
}
