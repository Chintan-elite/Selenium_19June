package statements;

public class S003_SwitchCase {
	public static void main(String[] args) {
		
		int choice = 1;
		
		switch(choice)
		{
		case 1 :
			System.out.println("You have choose Gujarati");
		case 2 :
			System.out.println("You have choose Hindi");
		case 3 : 
			System.out.println("You have choose English");
		}
		
		
		
	}
}
